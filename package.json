{
  "name": "prelude-wp",
  "version": "6.0.1",
  "description": "Prelude is a Wordpress starter theme that helps you craft custom themes. It uses Gulp to compile and minify scss/css, concatenate and minify JS, compress images, and more.",
  "main": "./util/moveFiles.js",
  "repository": "git@github.com:factor1/prelude-wp.git",
  "author": "Eric Stout <ericwstout@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "NODE_ENV=development gulp serve",
    "build": "yarn format && NODE_ENV=production gulp build",
    "test": "eslint .",
    "format": "prettier *.js *.css --write",
    "release-major": "yarn test && node ./util/versionUpdate.js --major && yarn build",
    "release-minor": "yarn test && node ./util/versionUpdate.js --minor && yarn build",
    "release-patch": "yarn test && node ./util/versionUpdate.js --patch && yarn build"
  },
  "bin": {
    "prelude-wp": "./bin/cli.js"
  },
  "dependencies": {
    "bowser": "^2.5.2",
    "fs-extra": "^8.1.0",
    "colors": "^1.3.3",
    "del": "^5.0.0",
    "dotenv": "^8.0.0",
    "lodash": "^4.17.15",
    "prompt": "^1.0.0",
    "replace": "^1.1.1",
    "slugify": "^1.3.4",
    "yargs": "^13.3.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "8",
    "browser-sync": "^2.26.7",
    "eslint": "^6.0.1",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-eslint": "^6.0.0",
    "gulp-imagemin": "^6.0.0",
    "gulp-mode": "^1.0.2",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "node-sass": "^4.12.0",
    "normalize-scss": "^7.0.1",
    "prettier": "^1.18.2"
  }
}
